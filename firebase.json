{
  "functions": [
    {
      "source": "functions_py",
      "codebase": "default",
      "ignore": [
        "venv",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    },
    {
      "codebase": "nodejs",
      "source": "functions",
      "runtime": "nodejs18"
    },
    {
      "source": "functions_js",
      "codebase": "functions_js",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ]
}
